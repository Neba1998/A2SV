/*
 * Complete the 'countSwaps' function below.
 *
 * The function accepts INTEGER_ARRAY a as parameter.
 */

function countSwaps(array) {
  for (let i = 0; i < array.length-1; i++){
      
     for(let j=0; j<array.length-1-i;j++){
        if(array[j]>array[j+1]){
            let tep=array[j];
            array[j]=array[j+1];
            array[j+1]=tep;
        } 
      } 
    }
       return array;
}
